n=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"map-block mbr-col-md-12 mbr-jc-c mbr-flex mbr-m-auto mbr-column\" mbr-class=\"{'mbr-col-lg-6':blockWidth==2,\n                         'mbr-col-lg-8':blockWidth==1,\n                         'mbr-col-lg-4':blockWidth==3}\">\n            <div class=\"box-block\">\n                <h3 class=\"mbr-fonts-style mbr-bold mbr-pb-3\" mbr-theme-style=\"display-5\">Contact Info</h3>\n                <p class=\"mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Dolorem, unde!</p>\n                <div class=\"mbr-pt-2\" mbr-if=\"showIcons\">\n                    <div class=\"iconfont-wrapper\">\n                        <span mbr-icon class=\"amp-iconfont animation-normal fa fa-youtube-play\"></span>\n                    </div>\n                    <div class=\"iconfont-wrapper\" mbr-if=\"iconsAmount > 1\">\n                        <span mbr-icon class=\"amp-iconfont animation-normal fa fa-twitter\"></span>\n                    </div>\n                    <div class=\"iconfont-wrapper\" mbr-if=\"iconsAmount > 2\">\n                        <span mbr-icon class=\"amp-iconfont animation-normal fa fa-facebook\"></span>\n                    </div>\n                    <div class=\"iconfont-wrapper\" mbr-if=\"iconsAmount > 3\">\n                        <span mbr-icon class=\"amp-iconfont animation-normal fa fa-instagram\"></span>\n                    </div>\n                </div>\n                <h4 class=\"mbr-fonts-style mbr-bold mbr-pt-2\" mbr-theme-style=\"display-7\">Address</h4>\n                <p>6834 Hollywood Blvd - Los Angeles CA</p>\n                <h5 class=\"mbr-fonts-style mbr-bold mbr-pt-2\" mbr-theme-style=\"display-7\">Contacts</h5>\n                <p>+1234 56 789</p>\n                <p>testmail@demo.com</p>\n            </div>\n            <div class=\"google-map\" mbr-map=\"googleMap\"></div>\n            <div class=\"map-placeholder mobirise-loader\">\n                <div placeholder class=\"placeholder\"></div>\n            </div>\n        </div>\n    </div>\n</section>",
