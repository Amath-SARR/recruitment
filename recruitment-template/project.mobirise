          "_customHTML": "<section class=\"tab1\" group=\"Tabs\" plugins=\"amp-main, amp-selector, amp-carousel\" mbr-class=\"{'mbr-fullscreen': fullScreen}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"2\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked=\"\">\n        <input type=\"color\" title=\"Active Color\" name=\"activetabColor\" value=\"#fffef2\">\n        <select name=\"tabsAmount\" title=\"Tabs Amount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\" selected=\"\">6</option>\n        </select>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/partneramp/components/_images/background-1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#dedede\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div class=\"container\">\n        <div class=\"mbr-row mbr-justify-content-center\">\n            <div class=\"mbr-col-lg-12 mbr-col-md-12 vcenter\">\n                <amp-selector role=\"tablist\" layout=\"container\" class=\"ampTabContainer\">\n                    <div role=\"tab\" class=\"tab-button\" selected=\"\" option=\"a\">\n                        <div class=\"card-img\">\n                            <a href=\"#\" target=\"_blank\" class=\"iconfont-wrapper\">\n                                <span mbr-icon=\"\" class=\"amp-iconfont animation-normal fa-th-list fa\"></span>\n                            </a>\n                        </div>\n                        <h3 class=\"tab-head align-center mbr-fonts-style mbr-semibold mbr-bold mbr-black\" mbr-theme-style=\"display-4\">\n                            Liste des<br>Candidatures</h3>\n                    </div>\n                    <div role=\"tabpanel\" class=\"tab-content\">\n                        <div class=\"mbr-col-lg-12\">\n                            <div class=\"mbr-row mbr-ai-c tab-wrapper\">\n                                <div class=\"text-block mbr-col-lg-6\">\n                                    <h5 class=\"mbr-fonts-style quoter-text mbr-text mbr-black\" mbr-theme-style=\"display-5\" data-app-selector=\".quoter-text\">\n                                        Candidatures</h5>\n                                    <p class=\"mbr-fonts-style text\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".text, .link-bottom\">\n                                        Lorem ipsum dolor sit\n                                        amet, consectetur adipiscing elit. Integer eget luctus turpis. Proin pretium elit lectus,\n                                        tempor facilisis nibh mollis non. In lacus felis, gravida ut consequat eu, sollicitudin sit\n                                        amet dolor. In auctor dui in metus pellentesque sagittis. Vivamus aliquet elementum purus,\n                                        nec mollis dolor finibus et.<br>\n                                    </p>\n                                    <h6 class=\"mbr-section-title mbr-fonts-style link-bottom mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".link-bottom\">\n                                        <a href=\"#\" class=\"text-primary animation-link\"><span class=\"read-more-arrow\"></span>READ MORE</a>\n                                    </h6>\n                                </div>\n                                <div class=\"img-block mbr-col-lg-6\">\n                                    <amp-img src=\"@PROJECT_PATH@/assets/images/people1.jpg\" alt=\"image\" height=\"409.688\" width=\"481.9858823529412\" layout=\"responsive\" class=\"mobirise-loader rounded__10\">\n                                        <a href=\"#\"><img src=\"@PROJECT_PATH@/assets/images/people1.jpg\" class=\"mbr-temp\"></a>\n                                    </amp-img>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                    <div role=\"tab\" class=\"tab-button\" option=\"b\">\n                        <div class=\"card-img\">\n                            <a href=\"#\" target=\"_blank\" class=\"iconfont-wrapper\">\n                                <span mbr-icon=\"\" class=\"amp-iconfont animation-normal mobi-mbri-star mobi-mbri\"></span>\n                            </a>\n                        </div>\n                        <h3 class=\"tab-head align-center mbr-fonts-style mbr-semibold mbr-bold mbr-black\" mbr-theme-style=\"display-4\">Profils<br> Intéressants</h3>\n                    </div>\n                    <div role=\"tabpanel\" class=\"tab-content\">\n                    