1\" value=\"0.3\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n\n<div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n   <div mbr-class=\"{'container-fluid' : fullWidth, 'container' : !fullWidth}\">\n        <div class=\"mbr-row wrapper\">\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 image\">\n                <amp-img src=\"@PROJECT_PATH@/assets/images/team3.jpg\" alt=\"image\" height=\"500\" width=\"450\" layout=\"responsive\" class=\"mobirise-loader\">\n                    <div placeholder class=\"placeholder\"></div>\n                    <img src=\"@PROJECT_PATH@/assets/images/team3.jpg\" class=\"mbr-temp\">\n                </amp-img>\n            </div>\n            <div class=\"mbr-col-sm-12 mbr-col-md-12 mbr-col-lg-6 mbr-flex right-block\">\n                <div class=\"text-block align-left\">\n                    <div class=\"title\">\n                        <h3 mbr-if=\"showTitle\" class=\"mbr-fonts-style card__title mbr-bold mbr-section-title mbr-white\" mbr-theme-style=\"display-5\" data-app-selector=\".title, .card__title\">\n                            Marry Clarins\n                        </h3>\n                        <h3 class=\"mbr-fonts-style card__text mbr-section-subtitle mbr-bold\" mbr-theme-style=\"display-4\" mbr-if=\"showSubTitle\" data-app-selector=\".card__text\">\n                            CHIEF CUSTOMER OFFICER\n                        </h3>\n                    </div>\n                   \n                    <div class=\"content__wrapper\" mbr-list mbr-list-grow=\"countService\" mbr-if=\"countService > 0\">\n                        <div class=\"text-wrapper\">\n                            <h6 class=\"mbr-fonts-style item__service\" mbr-theme-style=\"display-7\" data-app-selector=\".item__service, .social-list\">\n                                <span class=\"mbr-bold\">\n                                Position:</span>\n                                Chief Customer Officer\n                            </h6>\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h6 class=\"mbr-fonts-style item__service\" mbr-theme-style=\"display-7\" data-app-selector=\".item__service\">\n                                <span class=\"mbr-bold\">Responsibility:</span> Brand Development\n                            </h6>\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h6 class=\"mbr-fonts-style item__service\" mbr-theme-style=\"display-7\" data-app-selector=\".item__service\">\n                                <span class=\"mbr-bold\">Experience:</span> 10 Years\n                            </h6>\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h6 class=\"mbr-fonts-style item__service\" mbr-theme-style=\"display-7\" data-app-selector=\".item__service\">\n                                <span class=\"mbr-bold\">Email:</span> info@site.com\n                            </h6>\n                        </div>\n                        <div class=\"text-wrapper\">\n                            <h6 class=\"mbr-fonts-style item__service\" mbr-theme-style=\"display-7\" data-app-selector=\".item__service\">\n                                <span class=\"mbr-bold\">Phone:</span> +1 (800) 123 456 789\n                            </h6>\n                        </div>\n                    </div>\n                    <div class=\"social-list\" mbr-if=\"showSocial\" mbr-list mbr-list-grow=\"iconsCount\" mbr-link-type=\"socials\" data-app-selector=\".social-list\">\n                        <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\" class=\"iconfont-wrapper iconfont-wrapper-custom\">\n                            <span mbr-icon class=\"amp-iconfont fa-facebook-f fa\"></span>\n                        </a>  \n                      